(define (problem p01)
(:domain y-transport)
(:objects
server-loc monitor-loc goal-loc temp-loc  - location
truck-1 truck-2  - truck
capacity-0 capacity-1 capacity-2  - capacity-number
package-bag1 package-bag2  - package
)
(:init (road monitor-loc temp-loc)
(at-package package-bag2 server-loc)
(road temp-loc server-loc)
(at-truck truck-1 temp-loc)
(road temp-loc monitor-loc)
(no-goal monitor-loc)
(capacity truck-2 capacity-2)
(monitor package-bag1)
(goal goal-loc)
(at-truck truck-2 temp-loc)
(capacity-predecessor capacity-0 capacity-1)
(no-goal server-loc)
(no-goal temp-loc)
(road temp-loc goal-loc)
(road goal-loc temp-loc)
(capacity truck-1 capacity-1)
(at-package package-bag1 monitor-loc)
(road server-loc temp-loc)
(capacity-predecessor capacity-1 capacity-2)
(server package-bag2)
)
(:goal (and
	  (not (at-package package-bag1 temp-loc))
	  (not (at-package package-bag1 monitor-loc))
	  (not (at-package package-bag2 monitor-loc))
	  (not (at-package package-bag2 server-loc))
	  (not (in package-bag2 truck-1))
	  (at-package package-bag2 goal-loc)
	  (not (in package-bag1 truck-1))
	  (not (at-package package-bag2 temp-loc))
	  (not (at-package package-bag1 server-loc))
	  (not (in package-bag2 truck-2))
	  (not (in package-bag1 truck-2))
	  (at-package package-bag1 goal-loc))
)
)
